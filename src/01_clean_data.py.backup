                    df[col] = df[col].astype(str).str.replace('%', '').str.replace(',', '.').astype(float)
        
        # Add year column
        df['year'] = year
        
        return df
    except Exception as e:
        print(f"âŒ Error processing {year} data: {e}")
        return None

def main():
    """Main function to process all years of data"""
    print("ğŸ§¹ Starting data cleaning process...")
    
    # Create processed directory
    os.makedirs("data/processed/cleaned", exist_ok=True)
    
    # Find all years with processed data
    processed_years = []
    for year_dir in Path("data/processed").iterdir():
        if year_dir.is_dir() and year_dir.name.isdigit():
            processed_years.append(int(year_dir.name))
    
    print(f"ğŸ“… Found data for years: {sorted(processed_years)}")
    
    # Process data for each year
    all_data = []
    
    for year in sorted(processed_years):
        df = process_year_data(year)
        if df is not None:
            all_data.append(df)
    
    # Combine all years into one dataset
    if all_data:
        combined_df = pd.concat(all_data, ignore_index=True)
        
        # Save combined data
        output_path = "data/processed/cleaned/budget_execution_all_years.csv"
        combined_df.to_csv(output_path, index=False)
        print(f"ğŸ’¾ Saved combined data to {output_path}")
        
        # Also save individual year files
        for df in all_data:
            year = df['year'].iloc[0]
            year_output_path = f"data/processed/cleaned/budget_execution_{year}.csv"
            df.to_csv(year_output_path, index=False)
            print(f"ğŸ’¾ Saved {year} data to {year_output_path}")
        
        print(f"\nğŸ‰ Successfully processed {len(all_data)} years of data")
        print(f"ğŸ“Š Combined dataset has {len(combined_df)} rows and {len(combined_df.columns)} columns")
    else:
        print("âŒ No data was processed successfully")

if __name__ == "__main__":
    main()