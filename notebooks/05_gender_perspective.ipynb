   \"source\": [
    \"# Chart: Gender budget percentage by year\\nfig = go.Figure()\\n\\nfig.add_trace(go.Scatter(x=gender_summary['year'], y=gender_summary['gender_budget_pct'],\\n                         mode='lines+markers', name='Presupuesto %', line=dict(color='blue', width=4)))\\n\\nfig.add_trace(go.Scatter(x=gender_summary['year'], y=gender_summary['gender_execution_pct'],\\n                         mode='lines+markers', name='EjecuciÃ³n %', line=dict(color='red', width=4)))\\n\\nfig.add_hline(y=0, line_dash=\\\"dash\\\", line_color=\\\"gray\\\")\\n\\nfig.update_layout(\\n    title=\\\"Porcentaje de Gasto con Perspectiva de GÃ©nero\\\",\\n    xaxis_title=\\\"AÃ±o\\\",\\n    yaxis_title=\\\"% del Gasto Total\\\",\\n    showlegend=True\\n)\\n\\nfig.show()\\nfig.write_html(\\\"../public/charts/gender_spending_percentage.html\\\")\"
