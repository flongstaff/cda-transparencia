   \"source\": [
    \"# Chart: Gender budget by sector\\ngender_by_sector = budget_data.groupby('sector').agg({\\n    'gender_budget': 'sum',\\n    'gender_execution': 'sum',\\n    'budget': 'sum',\\n    'execution': 'sum'\\n}).reset_index()\\n\\ngender_by_sector['gender_budget_pct'] = (gender_by_sector['gender_budget'] / gender_by_sector['budget']) * 100\\ngender_by_sector['gender_execution_pct'] = (gender_by_sector['gender_execution'] / gender_by_sector['execution']) * 100\\n\\nfig = go.Figure()\\n\\nfig.add_trace(go.Bar(x=gender_by_sector['sector'], y=gender_by_sector['gender_budget_pct'],\\n                     name='Presupuesto %', marker_color='lightblue'))\\n\\nfig.add_trace(go.Bar(x=gender_by_sector['sector'], y=gender_by_sector['gender_execution_pct'],\\n                     name='EjecuciÃ³n %', marker_color='pink'))\\n\\nfig.update_layout(\\n    title=\\\"Porcentaje de Gasto con Perspectiva de GÃ©nero por Sector\\\",\\n    xaxis_title=\\\"Sector\\\",\\n    yaxis_title=\\\"% del Gasto del Sector\\\",\\n    barmode='group'\\n)\\n\\nfig.show()\\nfig.write_html(\\\"../public/charts/gender_spending_by_sector.html\\\")\"
   ]
