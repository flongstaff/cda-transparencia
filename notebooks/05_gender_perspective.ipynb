  {
   \"cell_type\": \"code\",
   \"execution_count\": null,
   \"metadata\": {},
   \"outputs\": [],
   \"source\": [
    \"# Donut chart for gender spending distribution\\ntotal_budget = gender_summary['budget'].sum()\\ntotal_execution = gender_summary['execution'].sum()\\ntotal_gender_budget = gender_summary['gender_budget'].sum()\\ntotal_gender_execution = gender_summary['gender_execution'].sum()\\n\\n# Calculate non-gender spending\\nnon_gender_budget = total_budget - total_gender_budget\\nnon_gender_execution = total_execution - total_gender_execution\\n\\nfig = make_subplots(\\n    rows=1, cols=2,\\n    subplot_titles=('Presupuesto', 'EjecuciÃ³n'),\\n    specs=[[{\\\"type\\\": \\\"domain\\\"}, {\\\"type\\\": \\\"domain\\\"}]]\\n)\\n\\nfig.add_trace(\\n    go.Pie(labels=['Con Perspectiva de GÃ©nero', 'Sin Perspectiva de GÃ©nero'], \\n           values=[total_gender_budget, non_gender_budget], \\n           name=\\\"Presupuesto\\\"),\\n    1, 1\\n)\\n\\nfig.add_trace(\\n    go.Pie(labels=['Con Perspectiva de GÃ©nero', 'Sin Perspectiva de GÃ©nero'], \\n           values=[total_gender_execution, non_gender_execution], \\n           name=\\\"EjecuciÃ³n\\\"),\\n    1, 2\\n)\\n\\nfig.update_layout(\\n    title_text=\\\"DistribuciÃ³n del Gasto: Con vs Sin Perspectiva de GÃ©nero\\\",\\n    # Add annotations for small percentages\\n    annotations=[\\n        dict(text=f'{(total_gender_budget/total_budget)*100:.3f}%', x=0.2, y=0.5, font_size=15, showarrow=False),\\n        dict(text=f'{(total_gender_execution/total_execution)*100:.3f}%', x=0.8, y=0.5, font_size=15, showarrow=False)\\n    ]\\n)\\n\\nfig.show()\\nfig.write_html(\\\"../public/charts/gender_spending_distribution.html\\\")\"
   ]
