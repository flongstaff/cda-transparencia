   \"source\": [
    \"# Chart: Gender budget vs execution by year\\ngender_summary = budget_data.groupby('year').agg({\\n    'gender_budget': 'sum',\\n    'gender_execution': 'sum',\\n    'budget': 'sum',\\n    'execution': 'sum'\\n}).reset_index()\\n\\ngender_summary['gender_budget_pct'] = (gender_summary['gender_budget'] / gender_summary['budget']) * 100\\ngender_summary['gender_execution_pct'] = (gender_summary['gender_execution'] / gender_summary['execution']) * 100\\n\\nfig = go.Figure()\\n\\n# Add bars for gender budget and execution\\nfig.add_trace(go.Bar(x=gender_summary['year'], y=gender_summary['gender_budget'],\\n                     name='Presupuesto de GÃ©nero', marker_color='lightblue'))\\n\\nfig.add_trace(go.Bar(x=gender_summary['year'], y=gender_summary['gender_execution'],\\n                     name='EjecuciÃ³n de GÃ©nero', marker_color='pink'))\\n\\nfig.update_layout(\\n    title=\\\"Gasto con Perspectiva de GÃ©nero por AÃ±o\\\",\\n    xaxis_title=\\\"AÃ±o\\\",\\n    yaxis_title=\\\"Monto (ARS)\\\",\\n    barmode='group'\\n)\\n\\nfig.show()\\nfig.write_html(\\\"../public/charts/gender_spending_by_year.html\\\")\"
   ]
  },
  {
   \"cell_type\": \"code\",
   \"execution_count\": null,
   \"metadata\": {},
